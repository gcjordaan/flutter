<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="uploadFilesForDebtCounsellingVM" type="com.gideonsapplication.app.modules.uploadfilesfordebtcounselling.data.viewmodel.UploadFilesForDebtCounsellingVM"/>
    </data>
    <LinearLayout android:id="@+id/linearUploadfilesfo"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <ImageView android:id="@+id/imageCroppedNBlogo"
            android:layout_gravity="start"
            android:layout_height="@dimen/_6pxh"
            android:layout_marginEnd="@dimen/_117pxh"
            android:layout_marginStart="@dimen/_117pxh"
            android:layout_width="@dimen/_119pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_croppednblogo"
            tools:ignore="ContentDescription" tools:src="@drawable/img_croppednblogo"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackconsolidationone"
            android:layout_height="@dimen/_126pxv"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageConsolidationOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_126pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_consolidation1_6}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_consolidation1_6"/>
            <FrameLayout android:id="@+id/frameStacklanguage"
                android:layout_gravity="start"
                android:layout_height="@dimen/_105pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearColumnlanguage"
                    android:layout_gravity="center"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_8pxh"
                    android:layout_width="match_parent"
                    android:orientation="vertical" style="@style/groupStylebluegray_100_a5">
                    <TextView android:gravity="center_horizontal"
                        android:id="@+id/txtLanguage"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="@dimen/_14pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_60pxv"
                        android:layout_width="match_parent"
                        android:text="@{uploadFilesForDebtCounsellingVM.uploadFilesForDebtCounsellingModel.txtLanguage}"
                        style="@style/txtPoppinsregular13" tools:text="@string/msg_to_apply_for_de2"/>
                </LinearLayout>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnSignUpForDebtCounselling"
                    android:layout_gravity="top|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_1pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/msg_sign_up_for_deb"
                    android:textAllCaps="false"
                    style="@style/btnSolid_1" tools:text="@string/msg_sign_up_for_deb"/>
            </FrameLayout>
        </FrameLayout>
        <TextView android:ellipsize="end"
            android:id="@+id/txtDocumentSubmis"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_110pxh"
            android:layout_marginStart="@dimen/_110pxh"
            android:layout_marginTop="@dimen/_9pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{uploadFilesForDebtCounsellingVM.uploadFilesForDebtCounsellingModel.txtDocumentSubmis}"
            style="@style/txtPoppinssemibold13" tools:text="@string/msg_document_submis"/>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineThree"
            android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_32pxv" android:layout_width="@dimen/_297pxh"/>
        <Spinner android:gravity="start"
            android:id="@+id/spinnerGroupNinetyThree"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowiddocumentbac"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_41pxh"
            android:layout_marginStart="@dimen/_41pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtIDDocumentBac"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_5pxv"
                android:layout_weight="0.85" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{uploadFilesForDebtCounsellingVM.uploadFilesForDebtCounsellingModel.txtIDDocumentBac}"
                style="@style/txtPoppinsregular13_2" tools:text="@string/msg_id_document_bac"/>
            <ImageView android:id="@+id/imageVectorOne"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_143pxh"
                android:layout_marginTop="@dimen/_3pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_indigo_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_indigo_900"/>
        </LinearLayout>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineFive"
            android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="@dimen/_297pxh"/>
        <Spinner android:gravity="start"
            android:id="@+id/spinnerGroupNinetyFive"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent" tools:listitem="@layout/spinner_item"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowproofofreside"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_40pxh"
            android:layout_marginStart="@dimen/_40pxh"
            android:layout_marginTop="@dimen/_8pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtProofofReside"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_weight="0.86" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{uploadFilesForDebtCounsellingVM.uploadFilesForDebtCounsellingModel.txtProofofReside}"
                style="@style/txtPoppinsregular13_2" tools:text="@string/msg_proof_of_reside"/>
            <ImageView android:id="@+id/imageVectorThree"
                android:layout_height="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_143pxh"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_indigo_900"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_indigo_900"/>
        </LinearLayout>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineSeven"
            android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_300pxh"/>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnvectorFour"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_25pxh"
            android:layout_marginStart="@dimen/_25pxh"
            android:layout_marginTop="@dimen/_62pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowvectorFour"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_17pxh"
                android:layout_marginStart="@dimen/_17pxh"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageButton
                    android:background="@drawable/rectangle_bg_green_700_border_green_700_radius_2_08"
                    android:id="@+id/btnVectorFour"
                    android:layout_height="@dimen/_16pxh"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_width="@dimen/_16pxh"
                    android:paddingHorizontal="@dimen/_3pxh"
                    android:paddingVertical="@dimen/_3pxv"
                    android:scaleType="fitCenter"
                    android:src="@drawable/img_vector_white_a700" tools:ignore="ContentDescription"/>
                <TextView android:id="@+id/txtTermAndCondition"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="0.94"
                    android:layout_width="0dp"
                    android:text="@{uploadFilesForDebtCounsellingVM.uploadFilesForDebtCounsellingModel.txtTermAndCondition}"
                    style="@style/txtPoppinsregular13_2" tools:text="@string/msg_i_have_read_and"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnApplyForDebtCounselling"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_66pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_25pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_25pxv"
                android:singleLine="true"
                android:text="@string/msg_apply_for_debt2"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_5" tools:text="@string/msg_apply_for_debt2"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageChatOne"
            android:layout_gravity="end"
            android:layout_height="@dimen/_45pxh"
            android:layout_marginBottom="@dimen/_8pxv"
            android:layout_marginEnd="@dimen/_5pxh"
            android:layout_marginStart="@dimen/_342pxh"
            android:layout_marginTop="@dimen/_11pxv"
            android:layout_width="@dimen/_44pxh"
            android:scaleType="fitXY" android:src="@drawable/img_chat1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_chat1"/>
    </LinearLayout>
</layout>
