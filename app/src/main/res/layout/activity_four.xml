<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="fourVM" type="com.gideonsapplication.app.modules.four.data.viewmodel.FourVM"/>
    </data>
    <LinearLayout android:id="@+id/linearFour"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <ImageView android:id="@+id/imageCroppedNBlogo"
            android:layout_gravity="start"
            android:layout_height="@dimen/_6pxh"
            android:layout_marginEnd="@dimen/_117pxh"
            android:layout_marginStart="@dimen/_117pxh"
            android:layout_width="@dimen/_119pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_croppednblogo"
            tools:ignore="ContentDescription" tools:src="@drawable/img_croppednblogo"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackwelcometoane"
            android:layout_height="@dimen/_210pxv"
            android:layout_marginTop="@dimen/_9pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageHeaderImageOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_210pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_headerimage1_1}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_headerimage1_1"/>
            <LinearLayout android:id="@+id/linearColumnwelcometoane"
                android:layout_gravity="bottom|end"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_70pxh"
                android:layout_marginStart="@dimen/_70pxh"
                android:layout_marginTop="@dimen/_14pxv"
                android:layout_width="match_parent" android:orientation="vertical">
                <TextView android:gravity="start"
                    android:id="@+id/txtWelcometoaNe"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:text="@{fourVM.fourModel.txtWelcometoaNe}"
                    style="@style/txtSourceSansProbold22" tools:text="@string/msg_welcome_to_a_ne"/>
                <TextView android:ellipsize="end"
                    android:gravity="start"
                    android:id="@+id/txtAbetterWayto"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_38pxh"
                    android:layout_marginStart="@dimen/_38pxh"
                    android:layout_marginTop="@dimen/_21pxv"
                    android:layout_width="match_parent"
                    android:singleLine="true"
                    android:text="@{fourVM.fourModel.txtAbetterWayto}"
                    style="@style/txtSourceSansProsemibold15" tools:text="@string/msg_a_better_way_to"/>
            </LinearLayout>
        </FrameLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtIDNUMBER"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_43pxh"
            android:layout_marginStart="@dimen/_43pxh"
            android:layout_marginTop="@dimen/_45pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{fourVM.fourModel.txtIDNUMBER}"
            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_id_number"/>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineThree"
            android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_21pxv" android:layout_width="@dimen/_297pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtFullnames"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_46pxh"
            android:layout_marginStart="@dimen/_46pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{fourVM.fourModel.txtFullnames}"
            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_full_names"/>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineFour"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_33pxh"
            android:layout_marginStart="@dimen/_33pxh"
            android:layout_marginTop="@dimen/_18pxv" android:layout_width="@dimen/_297pxh"/>
        <TextView android:ellipsize="end" android:id="@+id/txtSurname"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_45pxh"
            android:layout_marginStart="@dimen/_45pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{fourVM.fourModel.txtSurname}"
            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_surname"/>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineFive"
            android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_38pxh"
            android:layout_marginStart="@dimen/_38pxh"
            android:layout_marginTop="@dimen/_25pxv" android:layout_width="@dimen/_297pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtContactnumber"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{fourVM.fourModel.txtContactnumber}"
            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_contact_number"/>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineSix" android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_24pxv" android:layout_width="@dimen/_297pxh"/>
        <TextView android:ellipsize="end"
            android:id="@+id/txtEmailaddress"
            android:layout_gravity="start"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_37pxh"
            android:layout_marginStart="@dimen/_37pxh"
            android:layout_marginTop="@dimen/_22pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{fourVM.fourModel.txtEmailaddress}"
            style="@style/txtPoppinsregular13_1" tools:text="@string/lbl_e_mail_address"/>
        <View android:background="@color/black_900"
            android:id="@+id/lineLineSeven"
            android:layout_gravity="start"
            android:layout_height="@dimen/_1pxv"
            android:layout_marginEnd="@dimen/_35pxh"
            android:layout_marginStart="@dimen/_35pxh"
            android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_297pxh"/>
        <LinearLayout android:gravity="end"
            android:id="@+id/linearRowlineeight"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_28pxv"
            android:layout_marginEnd="@dimen/_15pxh"
            android:layout_marginStart="@dimen/_15pxh"
            android:layout_marginTop="@dimen/_17pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearColumnlock"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_27pxv"
                android:layout_weight="0.87" android:layout_width="0dp" android:orientation="vertical">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowlock"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginStart="@dimen/_3pxh"
                    android:layout_width="wrap_content" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageLock"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_width="@dimen/_16pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_lock_20x16"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_lock_20x16"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPassword"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_12pxh"
                        android:layout_marginTop="@dimen/_7pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@{fourVM.fourModel.txtPassword}"
                        style="@style/txtPoppinsregular13_1" tools:text="@string/lbl"/>
                </LinearLayout>
                <View android:background="@color/black_900"
                    android:id="@+id/lineLineEight"
                    android:layout_height="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_20pxv" android:layout_width="@dimen/_297pxh"/>
            </LinearLayout>
            <ImageView android:id="@+id/imageChatOne"
                android:layout_height="@dimen/_45pxh"
                android:layout_marginStart="@dimen/_2pxh"
                android:layout_marginTop="@dimen/_24pxv"
                android:layout_width="@dimen/_44pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_chat1"
                tools:ignore="ContentDescription" tools:src="@drawable/img_chat1"/>
        </LinearLayout>
    </LinearLayout>
</layout>
