<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="consolidateMyDebtStatusVM" type="com.gideonsapplication.app.modules.consolidatemydebtstatus.data.viewmodel.ConsolidateMyDebtStatusVM"/>
    </data>
    <LinearLayout android:id="@+id/linearConsolidatemy"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <ImageView android:id="@+id/imageCroppedNBlogo"
            android:layout_gravity="start"
            android:layout_height="@dimen/_6pxh"
            android:layout_marginEnd="@dimen/_117pxh"
            android:layout_marginStart="@dimen/_117pxh"
            android:layout_width="@dimen/_119pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_croppednblogo"
            tools:ignore="ContentDescription" tools:src="@drawable/img_croppednblogo"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackconsolidationone"
            android:layout_height="@dimen/_126pxv"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageConsolidationOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_126pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_consolidation1_2}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_consolidation1_2"/>
            <FrameLayout android:id="@+id/frameStackrectangle4194"
                android:layout_gravity="center"
                android:layout_height="@dimen/_105pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_390pxh">
                <ImageView android:id="@+id/imageRectangle4194"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_105pxh"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_width="@dimen/_383pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle4194}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4194"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnDebtConsolidationStatus"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/msg_debt_consolidat2"
                    android:textAllCaps="false"
                    style="@style/btnSolid_2" tools:text="@string/msg_debt_consolidat2"/>
            </FrameLayout>
        </FrameLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnclock"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <ImageView android:id="@+id/imageClock"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_32pxh"
                android:layout_marginEnd="@dimen/_73pxh"
                android:layout_marginStart="@dimen/_73pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_30pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_clock"
                tools:ignore="ContentDescription" tools:src="@drawable/img_clock"/>
            <TextView android:gravity="end"
                android:id="@+id/txtYourdocuments"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_42pxh"
                android:layout_marginStart="@dimen/_42pxh"
                android:layout_marginTop="@dimen/_23pxv"
                android:layout_width="match_parent"
                android:text="@{consolidateMyDebtStatusVM.consolidateMyDebtStatusModel.txtYourdocuments}"
                style="@style/txtPoppinsregular13" tools:text="@string/msg_your_documents"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnwecanaddsens"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <TextView android:ellipsize="end" android:gravity="end"
                android:id="@+id/txtWecanaddsens"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_60pxv"
                android:layout_marginEnd="@dimen/_45pxh"
                android:layout_marginStart="@dimen/_45pxh"
                android:layout_marginTop="@dimen/_49pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{consolidateMyDebtStatusVM.consolidateMyDebtStatusModel.txtWecanaddsens}"
                style="@style/txtPoppinsregular13_3" tools:text="@string/msg_we_can_add_sens"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnprice"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_83pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegray_800cornerRadius">
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtPrice"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_14pxv"
                android:layout_marginEnd="@dimen/_25pxh"
                android:layout_marginStart="@dimen/_25pxh"
                android:layout_marginTop="@dimen/_16pxv"
                android:layout_width="match_parent"
                android:text="@{consolidateMyDebtStatusVM.consolidateMyDebtStatusModel.txtPrice}"
                style="@style/txtPoppinsbold16" tools:text="@string/msg_earn_r100_off"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageChatOne"
            android:layout_gravity="end"
            android:layout_height="@dimen/_45pxh"
            android:layout_marginBottom="@dimen/_25pxv"
            android:layout_marginEnd="@dimen/_8pxh"
            android:layout_marginStart="@dimen/_339pxh"
            android:layout_marginTop="@dimen/_7pxv"
            android:layout_width="@dimen/_44pxh"
            android:scaleType="fitXY" android:src="@drawable/img_chat1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_chat1"/>
    </LinearLayout>
</layout>
