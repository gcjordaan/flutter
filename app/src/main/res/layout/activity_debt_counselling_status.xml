<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="debtCounsellingStatusVM" type="com.gideonsapplication.app.modules.debtcounsellingstatus.data.viewmodel.DebtCounsellingStatusVM"/>
    </data>
    <LinearLayout android:id="@+id/linearDebtCounsellin"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_100">
        <ImageView android:id="@+id/imageCroppedNBlogo"
            android:layout_gravity="start"
            android:layout_height="@dimen/_6pxh"
            android:layout_marginEnd="@dimen/_117pxh"
            android:layout_marginStart="@dimen/_117pxh"
            android:layout_width="@dimen/_119pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_croppednblogo"
            tools:ignore="ContentDescription" tools:src="@drawable/img_croppednblogo"/>
        <FrameLayout android:gravity="start"
            android:id="@+id/frameStackconsolidationone"
            android:layout_height="@dimen/_126pxv"
            android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
            <ImageView android:id="@+id/imageConsolidationOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_126pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_consolidation1_8}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_consolidation1_8"/>
            <FrameLayout android:id="@+id/frameStackrectangle4194"
                android:layout_gravity="center"
                android:layout_height="@dimen/_105pxv"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_11pxv" android:layout_width="@dimen/_390pxh">
                <ImageView android:id="@+id/imageRectangle4194"
                    android:layout_gravity="center"
                    android:layout_height="@dimen/_105pxh"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_width="@dimen/_383pxh"
                    android:scaleType="fitXY"
                    imageRes="@{@drawable/img_rectangle4194_3}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle4194_3"/>
                <androidx.appcompat.widget.AppCompatButton
                    android:ellipsize="end" android:gravity="center"
                    android:id="@+id/btnDebtCounsellingStatus"
                    android:layout_gravity="bottom|start"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_27pxv"
                    android:layout_marginTop="@dimen/_27pxv"
                    android:layout_width="match_parent"
                    android:paddingBottom="@dimen/_13pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_13pxv"
                    android:singleLine="true"
                    android:text="@string/msg_debt_counsellin3"
                    android:textAllCaps="false"
                    style="@style/btnSolid_1" tools:text="@string/msg_debt_counsellin3"/>
            </FrameLayout>
        </FrameLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnvector"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <ImageView android:id="@+id/imageVector"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_26pxh"
                android:layout_marginEnd="@dimen/_74pxh"
                android:layout_marginStart="@dimen/_74pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="@dimen/_26pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector_26x26"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector_26x26"/>
            <TextView android:gravity="end"
                android:id="@+id/txtLanguage"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_19pxv"
                android:layout_marginEnd="@dimen/_44pxh"
                android:layout_marginStart="@dimen/_44pxh"
                android:layout_marginTop="@dimen/_29pxv"
                android:layout_width="match_parent"
                android:text="@{debtCounsellingStatusVM.debtCounsellingStatusModel.txtLanguage}"
                style="@style/txtPoppinsregular13" tools:text="@string/msg_your_applicatio3"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearColumnwecanaddsens"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_16pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <TextView android:ellipsize="end" android:gravity="end"
                android:id="@+id/txtWecanaddsens"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/_60pxv"
                android:layout_marginEnd="@dimen/_45pxh"
                android:layout_marginStart="@dimen/_45pxh"
                android:layout_marginTop="@dimen/_49pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{debtCounsellingStatusVM.debtCounsellingStatusModel.txtWecanaddsens}"
                style="@style/txtPoppinsregular13_3" tools:text="@string/msg_we_can_add_sens"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearRowacceptOffer"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_80pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnAcceptOffer"
                android:layout_height="wrap_content"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_14pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_accept_offer"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_3" tools:text="@string/lbl_accept_offer"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnDeclineOffer"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_67pxh"
                android:layout_weight="0.5" android:layout_width="0dp"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_12pxh"
                android:paddingStart="@dimen/_12pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_decline_offer"
                android:textAllCaps="false"
                style="@style/btnSolidRounded_4" tools:text="@string/lbl_decline_offer"/>
        </LinearLayout>
        <ImageView android:id="@+id/imageChatOne"
            android:layout_gravity="end"
            android:layout_height="@dimen/_45pxh"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_35pxv"
            android:layout_width="@dimen/_44pxh"
            android:scaleType="fitXY" android:src="@drawable/img_chat1"
            tools:ignore="ContentDescription" tools:src="@drawable/img_chat1"/>
    </LinearLayout>
</layout>
